{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "public/assets/js",
    "public/assets/vendor/aos",
    "public/assets/vendor/bootstrap/js",
    "public/assets/vendor/glightbox/js",
    "public/assets/vendor/isotope-layout",
    "public/assets/vendor/php-email-form",
    "public/assets/vendor/swiper",
    "public/assets/vendor/waypoints"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "composite": false,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}